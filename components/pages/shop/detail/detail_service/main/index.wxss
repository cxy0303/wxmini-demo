.detial_service {
  min-height: 90vh;
  background-color: white;
  padding-bottom: 54px;
}

.detial_service .van-tabs {
  height: 100%;
}

.detial_service .from, .detial_service .to {
  padding: 15px 10px;
  font-size: 14px;
}

.detial_service .from span, .detial_service .to span {
  display: inline-block;
  width: 10px;
  height: 10px;
  border-radius: 50%;
  margin-right: 20px;
}

.detial_service .from span {
  background-color: #33d1c7;
}

.detial_service .to span {
  background-color: #d16b33;
}

.detial_service .time span {
  background-color: blue;
}

.detial_service .time p {
  float: right;
  color: #888;
  font-size: 18px;
}

.detial_service  .form {
  border-top: 1px solid #ddd;
  padding: 10px;
}

.detial_service  .form .fieldname {
  font-size: 12px;
}

.detial_service  .form input {
  border-bottom: 1px solid #eee;
  padding: 5px;
}

.detial_service .tips {
  font-size: 12px;
  color: #999;
  padding-top: 10px;
}

.detial_service_bottom {
  position: fixed;
  bottom: 0px;
  left: 0px;
  width: 100%;
  height: 50px;
  border-top: 1px solid #ddd;
  background-color: white;
}

.detial_service_bottom a {
  display: block;
  width: 100%;
  height: 100%;
  line-height: 50px;
  vertical-align: middle;
  text-align: center;
  font-size: 15px;
  color: #3347d1;
  font-weight: bolder;
}
