.scroll-side {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
}

.scroll-side scroll-view {
  width: 100%;
}

.scroll-side .container {
  padding: 0px 10px;
  word-break: keep-all;
  word-wrap: normal;
  white-space: nowrap;
}

.scroll-side .container building-item {
